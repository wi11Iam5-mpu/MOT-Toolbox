{
  "gui": {
    "resolution": [
      640,
      360
    ]
  },
  "video_player": {
    "fps": 100,
    "default_video_path": "./videos",
    "default_image_path": "./images"
  },
  "modules": [
    {
      "name": "VideoOverlayModule",
      "params": {
        "priority": 2,
        "default_depth_map_path": "./depth_maps",
        "split_position": 0.5
      }
    },
    {
      "name": "VideoAnnotationModule",
      "params": {
        "priority": 3,
        "default_annotation_path": "./annotations",
        "show_annotations": true,
        "confidence_threshold": 0.5
      }
    }
  ],
  "shortcuts": {
    "Play": "<space>",
    "Pause": "p",
    "Stop": "s",
    "LoadVideo": "o",
    "ToggleAnnotations": "a"
  }
}